{
  "name": "rl-web",
  "version": "1.0.0",
  "description": "Rocket League Replay Analysis Online",
  "main": "index.js",
  "scripts": {
    "type-check": "npx tsc --noEmit",
    "cy:run": "cypress run",
    "test": "start-server-and-test start http://localhost:1234 cy:run",
    "unit": "jest",
    "start": "./assets/build-wasm.sh && parcel src/index.html",
    "build": "./assets/build-wasm.sh --release && parcel build src/index.html"
  },
  "author": "",
  "license": "MIT",
  "jest": {
    "preset": "ts-jest"
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-preset-env": {
        "stage": 0
      }
    }
  },
  "browserslist": [
    "since 2018-01",
    "not kaios <= 2.5",
    "not android < 80"
  ],
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "cypress": "^3.8.3",
    "cypress-file-upload": "^3.5.3",
    "jest": "^25.1.0",
    "parcel-bundler": "^1.12.4",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "start-server-and-test": "^1.10.9",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "preact": "^10.3.3",
    "sanitize.css": "^11.0.0"
  }
}
