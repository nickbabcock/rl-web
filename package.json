{
  "name": "rl-web",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:wasm && next build",
    "build:wasm": "wasm-pack build -t web crate",
    "start": "next start",
    "lint": "next lint",
    "format": "npx prettier@latest --write 'src'",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "playwright test tests/e2e",
    "test:unit": "jest tests/unit"
  },
  "dependencies": {
    "comlink": "^4.3.1",
    "next": "12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "sanitize.css": "^13.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.21.1",
    "@types/node": "17.0.31",
    "@types/react": "17.0.43",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "jest": "^28.0.3",
    "typescript": "4.6.4"
  }
}
